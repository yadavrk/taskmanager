<%= button_to "Sign Up", '/users/new', method: :get%>
<h1>Login</h1>
<%= form_tag '/login' do %>
  <%= label_tag :email%><br>
  <%= text_field_tag :email %><br>
  <%= label_tag :password%><br>
  <%= password_field_tag :password%><br>
  <%= submit_tag "Login"%>
<%end%>